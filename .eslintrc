{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
      "no-empty": 1,
      "no-extra-semi": 2,
      "default-case": 2,
      "eqeqeq": 2,
      "semi": [2,"never"],
      "arrow-spacing": [1,{"before":true,"after":true}],
      "constructor-super": 2,
      "no-this-before-super": 2,
      "prefer-const": 1
    }
}